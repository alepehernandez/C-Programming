/* Write a C program that ask users to input two numbers, then add the two numbers*/

#include <stdio.h>
int main()
{
    // define integers a and b
    int a,b;
    //Ask users for number inout then Read the numbers usign scanf
    printf("Enter two numbers:");
    scanf("%d %d", &a, &b);
    
    printf("The sum of the two number is: %d\n", a+b);
    return 0;
}
