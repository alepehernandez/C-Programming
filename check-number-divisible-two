#inclyde <stdio.h>
int main()
{
  int num;
    print ("Enter a number: ");
    scanf("%d, &num);
    int divisible = num%2;
        
        if (divisible ==0)
        {
        print ("%d is divisible by 2/n", num);
        }
        else
        {
        printf("%d is not divisible by 2\n", num);
        }
